# 智诊通前端系统环境配置示例

# API基础URL
VITE_API_BASE_URL=http://localhost:8000

# 应用环境
VITE_APP_ENV=development

# 应用名称
VITE_APP_NAME=智诊通系统

# 应用版本
VITE_APP_VERSION=1.0.0

# 调试模式
VITE_DEBUG=true

# 日志级别
VITE_LOG_LEVEL=info

# 文件上传配置
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,audio/wav,audio/mp3

# 功能开关
VITE_ENABLE_REAL_TIME_CHAT=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_VOICE_INPUT=true
VITE_ENABLE_IMAGE_RECOGNITION=true


